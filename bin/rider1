#!/bin/sh
#Generated by JetBrains Toolbox 1.20.8804 at 2021-07-06T09:04:19.386229

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/davidlewis/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-1/211.7442.29/Rider.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"